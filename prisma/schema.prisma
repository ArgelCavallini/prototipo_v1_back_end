// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id String @id @default(uuid())
  nome String 
  sobrenome String
  cpf Int @unique
  e_mail String
  telefone_fixo Int?
  telefone_movel Int?
  cep Int
  ativo Boolean @default(true)

  @@map("cliente")
}
